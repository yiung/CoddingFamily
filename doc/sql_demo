-- 根据实际

CREATE TABLE `e_account` (
  `id` bitint(20) NOT NULL COMMENT '账号ID，使用雪花算法',
  `account` varchar(32) DEFAULT NULL COMMENT '账号',
  `password` varchar(64) DEFAULT NULL COMMENT '密码',
  `state` tinyint(2) NOT NULL COMMENT '启用状态 0:禁用,1:启用',
  `user_id` bitint(20) DEFAULT NULL COMMENT '用户ID',
  `gmt_create` date NOT NULL COMMENT '创建时间',
  `gmt_modified` date NOT NULL COMMENT '创建时间',
  PRIMARY KEY (`id`) USING BTREE,
)DEFAULT CHARSET=utf8 COMMENT='账号表'


CREATE TABLE `t_account_configure` (
  `account_id` bitint(20) NOT NULL COMMENT '账号ID',
  `platform` varchar(30) DEFAULT NULL COMMENT '客服端',
  `device_id` varchar(50) DEFAULT NULL COMMENT '设备ID',
  `model` varchar(30) DEFAULT NULL COMMENT '手机型号',
  `os_version` varchar(30) DEFAULT NULL COMMENT '操作系统版本',
  `manufacturer` varchar(100) DEFAULT NULL COMMENT '厂商',
  `version` varchar(20) DEFAULT NULL COMMENT '甲丁版本',
  `default_role` varchar(32) DEFAULT NULL COMMENT '默认角色',
  `default_role_name` varchar(20) DEFAULT NULL COMMENT '角色名',
  `default_pro_code` varchar(10) DEFAULT NULL COMMENT '默认项目',
  `gps_refresh_time` int(11) DEFAULT '30' COMMENT 'gps刷新间隔时间',
  `beat_interval` int(11) DEFAULT '0' COMMENT '心跳间隔时间',
  `clockin_count` int(2) DEFAULT '1' COMMENT '规定打卡次数',
  `attend_config_id` bigint(20) DEFAULT NULL COMMENT '默认考勤组配置id',
  `enable_check` tinyint(1) DEFAULT '1' COMMENT '默认用户是否打卡（1是/0不是）',
  PRIMARY KEY (`account_id`) USING BTREE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='用户默认配置'


